{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "target": "es6",
    "skipLibCheck": true,
    "outDir": "dist",
    "strictNullChecks": true
  },
  "include": [
    "scripts",
  ]
}